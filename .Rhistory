library(vcdExtra)
library(dplyr)
library(ggplot2)
library(ggmosaic)
data <- read.csv2('C:\\Users\\Filip\\Desktop\\ANKIETYZACJA DANYCH\\personel.csv', header = TRUE)
colnames(data) <- c('D','S','A1','A2','W1', 'W2', 'P', 'Wiek', 'Wyk')
#####################
View(data)
table(data['A1'])
data %>% group_by(A1) %>% count()
################
